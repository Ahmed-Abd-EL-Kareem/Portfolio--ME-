{
  "name": "3d-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:a11y": "jest --testPathPattern=a11y",
    "test:performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "test:seo": "lighthouse http://localhost:3000 --only-categories=seo --output=json --output-path=./seo-report.json",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "lighthouse:ci": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-ci.json",
    "pagespeed": "npx psi http://localhost:3000",
    "pagespeed:detailed": "npx psi http://localhost:3000 --strategy=mobile",
    "webpagetest": "npx webpagetest http://localhost:3000",
    "compare-performance": "echo 'lighthouse-compare removed'",
    "monitor-cwv": "npx web-vitals",
    "performance-report": "npx lighthouse http://localhost:3000 --output=json | jq '.audits'",
    "optimize-images": "npx imagemin public/images/* --out-dir=public/images/optimized",
    "convert-webp": "npx imagemin public/images/* --plugin=webp --out-dir=public/images/webp",
    "compress-images": "npx imagemin public/images/* --plugin=mozjpeg --out-dir=public/images/compressed",
    "purge-css": "npx purgecss --css ./app/globals.css --content ./app/**/*.{js,ts,jsx,tsx} --output ./app/purged.css",
    "optimize-css": "npx cssnano ./app/globals.css -o ./app/optimized.css",
    "bundle-analyzer": "ANALYZE=true npm run build",
    "remove-unused": "npx depcheck",
    "optimize-bundle": "npx webpack-bundle-analyzer .next/static/chunks/*.js",
    "network-analyzer": "npx webpack-bundle-analyzer .next/static/chunks/*.js --mode=network",
    "performance-analyzer": "npx webpack-bundle-analyzer .next/static/chunks/*.js --mode=performance",
    "detailed-report": "npx lighthouse http://localhost:3000 --output=json --output-path=./detailed-report.json",
    "memory-analyzer": "node --inspect npm run dev",
    "memory-leak-detector": "npx memlab run",
    "optimize-memory": "npx memlab analyze",
    "check-seo": "npx lighthouse http://localhost:3000 --only-categories=seo",
    "generate-sitemap": "npx next-sitemap",
    "check-links": "npx broken-link-checker http://localhost:3000",
    "check-contrast": "npx axe-core",
    "test:keyboard": "npx axe-core --rules=keyboard",
    "clean": "rm -rf .next node_modules/.cache",
    "reset": "rm -rf .next node_modules package-lock.json && npm install",
    "health-check": "npm run lint && npm run type-check && npm run test",
    "monitor": "npx lighthouse http://localhost:3000 --output=json --output-path=./monitor.json",
    "export": "next build && next export",
    "build:css": "npx tailwindcss -i ./app/globals.css -o ./app/output.css --watch",
    "preview": "next build && next start",
    "deploy": "npm run build && vercel --prod",
    "deploy:staging": "npm run build && vercel",
    "backup": "git add . && git commit -m 'backup before changes'",
    "update-deps": "npm update && npm audit fix",
    "security-audit": "npm audit && npm audit fix",
    "performance-audit": "npm run lighthouse && npm run pagespeed",
    "optimize": "node scripts/performance-optimize.js",
    "optimize-images": "node scripts/optimize-images.js",
    "accessibility-audit": "npm run test:a11y && npm run check-contrast",
    "seo-audit": "npm run check-seo && npm run generate-sitemap",
    "full-audit": "npm run security-audit && npm run performance-audit && npm run accessibility-audit && npm run seo-audit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/scroll-mode": "^3.12.0",
    "@react-pdf-viewer/search": "^3.12.0",
    "@react-pdf-viewer/selection-mode": "^3.12.0",
    "@react-pdf-viewer/thumbnail": "^3.12.0",
    "@react-pdf-viewer/toolbar": "^3.12.0",
    "@react-pdf-viewer/zoom": "^3.12.0",
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "next": "14.2.5",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^2.0.0",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-modules-commonjs": "^7.23.0",
    "@eslint/eslintrc": "^3.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.158.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.8.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "broken-link-checker": "^0.7.8",
    "cssnano": "^6.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.51.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jq": "^1.0.0",
    "lighthouse": "^11.4.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "psi": "^4.0.0",
    "purgecss": "^5.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "web-vitals": "^3.5.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpagetest": "^0.3.8"
  }
}
